FROM nginx

RUN echo "1" >> /usr/share/nginx/html/test.txt
RUN echo "2" >> /usr/share/nginx/html/test.txt
RUN echo "2" >> /usr/share/nginx/html/test.txt

RUN \
echo "1" >> /usr/share/nginx/html/test1.txt && \
echo "2" >> /usr/share/nginx/html/test1.txt && \
echo "2" >> /usr/share/nginx/html/test1.txt

RUN echo "1" > /usr/share/nginx/html/index.html